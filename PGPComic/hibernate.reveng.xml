<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>



	<table-filter match-catalog="mydb" match-name="comic" />
	<table-filter match-name="mediodepublicacion"
		match-catalog="mydb">
	</table-filter>
	<table-filter match-name="publicacion" match-catalog="mydb"></table-filter>
	<table-filter match-name="serie" match-catalog="mydb"></table-filter>
	<table-filter match-name="serie_has_mediodepublicacion"
		match-catalog="mydb">
	</table-filter>
</hibernate-reverse-engineering>